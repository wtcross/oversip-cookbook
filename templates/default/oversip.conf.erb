core:
  nameservers: <%= @core_conf['nameservers'].empty? ? 'null' : @core_conf['nameservers'].to_s.gsub(/"/, '') %>
  syslog_facility: <%= @core_conf['syslog_facility'] %>
  syslog_level: <%= @core_conf['syslog_level'] %>

sip:
  sip_udp: <%= @sip_conf['sip_udp'] %>
  sip_tcp: <%= @sip_conf['sip_tcp'] %>
  sip_tls: <%= @sip_conf['sip_tls'] %>
  enable_ipv4: <%= @sip_conf['enable_ipv4'] %>
  listen_ipv4: <%= @sip_conf['listen_ipv4'] %>
  advertised_ipv4: <%= @sip_conf['advertised_ipv4'] %>
  enable_ipv6: <%= @sip_conf['enable_ipv6'] %>
  listen_ipv6: <%= @sip_conf['listen_ipv6'] %>
  advertised_ipv6: <%= @sip_conf['advertised_ipv6'] %>
  listen_port: <%= @sip_conf['listen_port'] %>
  listen_port_tls: <%= @sip_conf['listen_port_tls'] %>
  use_tls_tunnel: <%= @sip_conf['use_tls_tunnel'] %>
  listen_port_tls_tunnel: <%= @sip_conf['listen_port_tls_tunnel'] %>
  callback_on_client_tls_handshake: <%= @sip_conf['callback_on_client_tls_handshake'] %>
  tcp_keepalive_interval: <%= @sip_conf['tcp_keepalive_interval'] %>
  record_route_hostname_tls_ipv4: <%= @sip_conf['record_route_hostname_tls_ipv4'] %>
  record_route_hostname_tls_ipv6:  <%= @sip_conf['record_route_hostname_tls_ipv6'] %>
  local_domains: <%= @sip_conf['local_domains'].empty? ? 'null' : @core_conf['local_domains'].to_s.gsub(/"/, '') %>

websocket:
  sip_ws: <%= @ws_conf['sip_ws'] %>
  sip_wss: <%= @ws_conf['sip_wss'] %>
  enable_ipv4: <%= @ws_conf['enable_ipv4'] %>
  listen_ipv4: <%= @ws_conf['listen_ipv4'] %>
  advertised_ipv4: <%= @ws_conf['advertised_ipv4'] %>
  enable_ipv6: <%= @ws_conf['enable_ipv6'] %>
  listen_ipv6: <%= @ws_conf['listen_ipv6'] %>
  advertised_ipv6: <%= @ws_conf['advertised_ipv6'] %>
  listen_port: <%= @ws_conf['listen_port'] %>
  listen_port_tls: <%= @ws_conf['listen_port_tls'] %>
  use_tls_tunnel: <%= @ws_conf['use_tls_tunnel'] %>
  listen_port_tls_tunnel: <%= @ws_conf['listen_port_tls_tunnel'] %>
  callback_on_client_tls_handshake: <%= @ws_conf['callback_on_client_tls_handshake'] %>
  max_ws_message_size: <%= @ws_conf['max_ws_message_size'] %>
  max_ws_frame_size: <%= @ws_conf['max_ws_frame_size'] %>
  ws_keepalive_interval: <%= @ws_conf['ws_keepalive_interval'] %>

tls:
  public_cert: <%= @tls_conf['public_cert'] %>
  private_cert: <%= @tls_conf['private_cert'] %>
  ca_dir: <%= @tls_conf['ca_dir'] %>